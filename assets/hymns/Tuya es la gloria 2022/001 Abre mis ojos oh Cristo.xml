<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="../lib/openlyrics/openlyrics.css" type="text/css"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8">
	<properties>
			<titles>
				<title>001 Abre mis ojos oh Cristo</title>
			</titles>
			<authors>
				<author>Tuya es la Gloria</author>
			</authors>					
			<publisher>Editorial Siembra</publisher>
			<songbooks>
				<songbook name="Tuya es la Gloria" entry="1"/>
			</songbooks>					
			</properties>
			<lyrics>
		<verse>
				<lines>Abre mis ojos, ¡oh, Cristo!<br/>Abre mis ojos, Señor,<br/>yo quiero verte, yo quiero verte.<br/> <br/>Yo quiero verte enaltecido, ¡oh, Dios!<br/>radiante en toda tu gloria.<br/>Derrama hoy tu poder<br/>sobre tu pueblo,<br/>Padre Santo.<br/>/Santo, santo, santo./<br/>Yo quiero verte.</lines>
			</verse>
	</lyrics>
</song>