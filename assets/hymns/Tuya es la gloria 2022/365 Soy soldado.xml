	<properties>
			<titles>
				<title>365 Soy soldado</title>
			</titles>
			<authors>
				<author>Tuya es la Gloria</author>
			</authors>					
			<publisher>Editorial Siembra</publisher>
			<songbooks>
				<songbook name="Tuya es la Gloria" entry="365"/>
			</songbooks>					
			</properties>
			<lyrics>
		<verse>
				<lines>/Soy soldado, soy soldado,<br/>soy soldado del ejército del Señor./<br/> <br/>Aunque no soy de la infantería,<br/>caballería, artillería,<br/>aunque en avión no vaya volando, <br/>pero soldado soy.<br/>/Soldado soy de Jesús./<br/></lines>
			</verse>
	</lyrics>
