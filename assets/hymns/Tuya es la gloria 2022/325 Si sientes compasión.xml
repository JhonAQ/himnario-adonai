	<properties>
			<titles>
				<title>325 Si sientes compasión</title>
			</titles>
			<authors>
				<author>Tuya es la Gloria</author>
			</authors>					
			<publisher>Editorial Siembra</publisher>
			<songbooks>
				<songbook name="Tuya es la Gloria" entry="325"/>
			</songbooks>					
			</properties>
			<lyrics>
		<verse>
				<lines>/Si sientes compasión por tus amigos,<br/>entonces, háblales de tu Señor./<br/> <br/>/Háblales de Cristo; cuenta a tus amigos,<br/>lo que el Salvador hizo por ti./<br/> <br/>/Testifica, testifica,<br/>testifica de Cristo por doquier./<br/></lines>
			</verse>
	</lyrics>
