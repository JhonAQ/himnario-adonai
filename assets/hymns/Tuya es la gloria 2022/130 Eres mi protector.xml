<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8">
	<properties>
			<titles>
				<title>130 Eres mi protector</title>
			</titles>
			<authors>
				<author>Tuya es la Gloria</author>
			</authors>					
			<publisher>Editorial Siembra</publisher>
			<songbooks>
				<songbook name="Tuya es la Gloria" entry="130"/>
			</songbooks>					
			</properties>
			<lyrics>
		<verse>
				<lines>Eres mi Protector, llenas mi corazón <br/>con cánticos de liberación, <br/>de angustias me guardarás.<br/> <br/>	Confiaré en ti, confiaré en ti, <br/>	el débil dirá: “Fuerte soy <br/>	con poder del Señor.”<br/></lines>
			</verse>
	</lyrics>
</song>