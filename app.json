{
  "expo": {
    "name": "Himnario Adonai",
    "description": "Himnario Oficial para las reuniones del grupo Adonai",
    "slug": "himnario-adonai",
    "version": "1.30.0",
    "orientation": "portrait",
    "icon": "./assets/images/adonai-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/adonai-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jhonatanarias.himnarioadonai"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-sqlite",
        {
          "enableFTS": true
        }
      ],
      "expo-asset"
    ],
    "githubUrl": "https://github.com/JhonAQ/himnario-adonai",
    "extra": {
      "eas": {
        "projectId": "5650ead3-2854-40ac-984d-764233e0f8c2"
      }
    },
    "assetBundlePatterns": [
      "assets/**/*",
      "assets/database/*"
    ]
  }
}