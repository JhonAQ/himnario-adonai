<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="../lib/openlyrics/openlyrics.css" type="text/css"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.9">
  <properties>
    <titles>
      <title>Sana nuestra tierra</title>
    </titles>
    <authors>
      <author>Desconocido</author>
    </authors>
    <publisher>Editorial Siembra</publisher>
    <songbooks>
      <songbook name="Tuya es la Gloria 2022" entry="309"/>
    </songbooks>
    <themes>
      <theme>Arrepentimiento y Confesión</theme>
      <theme>Oración y Devoción Personal</theme>
    </themes>
    <keywords>
      <keyword>Arrepentimiento</keyword>
      <keyword>Sanidad</keyword>
      <keyword>Perdón</keyword>
      <keyword>Humildad</keyword>
      <keyword>Oración</keyword>
    </keywords>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Vengo a ti, guíame,<br/>santifícame en ti.<br/>Quiero andar en tu verdad.<br/>Tómame, abrázame,<br/>mi corazón hoy vuelve a ti,<br/>solo a ti.</lines>
    </verse>
    <verse name="v2">
      <lines>Quiero humillarme, buscar tu rostro.<br/>Hoy me arrepiento delante de ti.<br/>Vengo a invocarte, inclina tu oído.<br/>Escucha y perdona mi rebelión.</lines>
    </verse>
    <verse name="c">
      <lines>//Sana nuestra tierra//<br/>con tu amor.</lines>
    </verse>
  </lyrics>
</song>