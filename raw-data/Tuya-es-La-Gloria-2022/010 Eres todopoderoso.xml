<?xml version="1.0" encoding="UTF-8"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.9">
  <properties>
    <titles>
      <title>Eres todopoderoso</title>
    </titles>
    <authors>
      <author>Desconocido</author>
    </authors>
    <songbooks>
      <songbook name="Tuya es la gloria 2022" entry="10"/>
    </songbooks>
    <publisher>Editorial Siembra</publisher>
    <themes>
      <theme>Adoración y Alabanza</theme>
      <theme>Consagración y Servicio</theme>
    </themes>
    <keywords>
      <keyword>Adoración</keyword>
      <keyword>Jesús</keyword>
      <keyword>Señor</keyword>
      <keyword>Salvación</keyword>
      <keyword>Majestuoso</keyword>
    </keywords>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>La única razón de mi adoración<br/>eres tú, mi Jesús.<br/>El único motivo para vivir<br/>eres tú, mi Señor.<br/>Mi única verdad está en ti,<br/>eres mi luz y mi salvación.<br/>Mi único amor eres tú, Señor,<br/>y por siempre te alabaré.</lines>
    </verse>
    <verse name="c">
      <lines>Tú eres todopoderoso,<br/>eres grande y majestuoso,<br/>eres fuerte, invencible,<br/>y no hay nadie como tú.</lines>
    </verse>
  </lyrics>
</song>
