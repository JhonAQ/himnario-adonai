    <titles>
      <title>Perseveremos hermanos</title>
    </titles>
    <authors>
      <author>Desconocido</author>
    </authors>
    <songbooks>
      <songbook name="Tuya es la Gloria 2022" entry="300"/>
    </songbooks>
    <publisher>Editorial Siembra</publisher>
    <themes>
      <theme>Esperanza y Segunda Venida</theme>
      <theme>Consagración y Servicio</theme>
    </themes>
    <keywords>
      <keyword>Perseverancia</keyword>
      <keyword>Fe</keyword>
      <keyword>Venida</keyword>
      <keyword>Señor</keyword>
      <keyword>Hermanos</keyword>
    </keywords>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>/Perseveremos hermanos<br/>en la fe, en la fe.<br/>La venida del Señor<br/>cerca está, cerca está./<br/></lines>
    </verse>
    <verse name="v2">
      <lines>Hermano mío, nunca desmayes,<br/>cuando la gente habla contra ti.<br/>Mejor procurar mirar al cielo.<br/>/El Señor Jesús está contigo./<br/></lines>
    </verse>
  </lyrics>
</song>