<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.9">
  <properties>
    <titles>
      <title>Mucho porque vivir</title>
    </titles>
    <authors>
      <author>Desconocido</author>
    </authors>
    <publisher>Editorial Siembra</publisher>
    <songbooks>
      <songbook name="Tuya es la Gloria 2022" entry="296"/>
    </songbooks>
    <themes>
      <theme>Vida de Cristo</theme>
      <theme>Esperanza y Segunda Venida</theme>
      <theme>Consagración y Servicio</theme>
    </themes>
    <keywords>
      <keyword>vivir</keyword>
      <keyword>Cristo</keyword>
      <keyword>paz</keyword>
      <keyword>amor</keyword>
      <keyword>felicidad</keyword>
    </keywords>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>/Mucho porque vivir,<br/>Cristo tiene mucho que darte./<br/></lines>
    </verse>
    <verse name="c">
      <lines>/Te da la paz, te da el amor,<br/>te da la felicidad./<br/></lines>
    </verse>
    <verse name="v2">
      <lines>/Él tiene para ti,<br/>lo que tú no has encontrado./<br/></lines>
    </verse>
    <verse name="v3">
      <lines>/Entrégale tu ser<br/>y podrás apreciar la vida./<br/></lines>
    </verse>
  </lyrics>
</song>